        {
          "key": "Permissions-Policy",
          "value": "camera=*, microphone=*, display-capture=*, autoplay=*"
        }
      ]
    }
  ],
  "crons": [
    {
      "path": "/api/cron/subscription-check",
      "schedule": "0 9 * * *"
    }
  ]
